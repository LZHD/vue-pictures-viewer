{
  "name": "vue-pictures-viewer",
  "version": "0.0.1",
  "private": false,
  "main": "publish/vue-pictures-viewer.umd.min.js",
  "description": "vue-pictures-viewer based on element-ui and iv-viewer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lzhd/vue-pictures-viewer.git"
  },
  "keywords": [
    "image-viewer",
    "image",
    "vue",
    "pictures-viewer",
    "viewer",
    "element-ui"
  ],
  "author": "lzhd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lzhd/vue-pictures-viewer/issues"
  },
  "homepage": "https://lzhd.github.io/vue-pictures-viewer",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --target lib ./lib/index.js",
    "lint": "vue-cli-service lint",
    "site:build": "vue-cli-service build --dest dist",
    "site:gh": "npm run site:build && gh-pages -d ./dist",
    "release:next": "npm run build && npm publish --access public --tag next",
    "release": "npm run build && npm publish --access public"
  },
  "dependencies": {
    "element-ui": ">=2.10.0",
    "iv-viewer": ">=2.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.6",
    "@vue/cli-plugin-eslint": "~4.5.6",
    "@vue/cli-service": "~4.5.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "files":["package.json","publish/vue-pictures-viewer.umd.min.js"]
}
